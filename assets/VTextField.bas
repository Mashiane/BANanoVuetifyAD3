Sub FormatDateTime(C As VueComponent)
	Dim res1 As String = C.GetData(dateModel)
	Dim res2 As String = C.GetData(timeModel)
	Dim xDate As String = ""
	If res1 <> "" Then
		xDate = C.NiceDate(res1)
	End If
	Dim res As String = $"${xDate} ${res2}"$
	res = res.Trim
	Dim resx As String = $"${sVModel}fmt"$
	C.SetData(resx, res)
