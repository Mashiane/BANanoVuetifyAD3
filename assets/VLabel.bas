Sub UpdateVisible(C As VueComponent, b As Boolean) As VLabel
	C.SetData(sVIf, b)
	C.SetData(sVShow, b)
	Return Me
End Sub

Sub UpdateVisibleOnApp(C As VuetifyApp, b As Boolean) As VLabel
	C.SetData(sVIf, b)
	C.SetData(sVShow, b)
	Return Me
End Sub

