Sub Initialize (CallBack As Object, Name As String, EventName As String)
	mName = Name.tolowercase
	mEventName = EventName.ToLowerCase
	mCallBack = CallBack	
	mName = mName.Replace("#","")
	mEventName = mEventName.Replace("#","")
	If mName <> "" Then
		Dim fKey As String = $"#${mName}"$
		If BANano.Exists(fKey) Then 
			mElement = BANano.GetElement(fKey)
		End If
	End If
	xColor = $"${mName}color"$
	sVShow = $"${mName}show"$
	xTitle = $"${mName}caption"$
End Sub
	
Sub DesignerCreateView (Target As BANanoElement, Props As Map)
	mTarget = Target
	If Props <> Null Then
		mClasses = Props.Get("Classes")
		mStyles = Props.Get("Styles")
		mAttributes = Props.Get("Attributes")
		bAbsolute = Props.Get("Absolute")
		bBottom = Props.Get("Bottom")
		bCollapse = Props.Get("Collapse")
		sColor = Props.Get("Color")
		sColorIntensity = Props.Get("ColorIntensity")
		bDark = Props.Get("Dark")
		bDense = Props.Get("Dense")
		sElevation = Props.Get("Elevation")
		bExtended = Props.Get("Extended")
		sExtensionHeight = Props.Get("ExtensionHeight")
		bFlat = Props.Get("Flat")
		bFloating = Props.Get("Floating")
		sHeight = Props.Get("Height")
		bLight = Props.Get("Light")
		sMaxHeight = Props.Get("MaxHeight")
		sMaxWidth = Props.Get("MaxWidth")
		sMinHeight = Props.Get("MinHeight")
		sMinWidth = Props.Get("MinWidth")
		bOutlined = Props.Get("Outlined")
		bProminent = Props.Get("Prominent")
		sRounded = Props.Get("Rounded")
		bShaped = Props.Get("Shaped")
		bShort = Props.Get("Short")
		sSrc = Props.Get("Src")
		sTextColor = Props.Get("TextColor")
		sTextColorIntensity = Props.Get("TextColorIntensity")
		bTile = Props.Get("Tile")
		sVIf = Props.Get("VIf")
		bHidden = Props.GetDefault("Hidden", False)
		sWidth = Props.Get("Width")
		sSearchVModel = Props.getdefault("SearchVModel", "")
		bHasTitle = Props.GetDefault("HasTitle", False)
		bHasTitle = BANanoShared.parseBool(bHasTitle)
		sTitle = Props.GetDefault("Title", "")
		mAlign = Props.Get("Align")
		mJustify = Props.Get("Justify")
		bDividersBetween = Props.GetDefault("DividersBetween", False)
		bDividersBetween = BANanoShared.parseBool(bDividersBetween)
		'
		bHasAddnew = Props.GetDefault("HasAddnew", False)
		bHasAddnew = BANanoShared.parseBool(bHasAddnew)
		
		bHasBack = Props.GetDefault("HasBack", False)
		bHasBack = BANanoShared.parseBool(bHasBack)
		
		bHasClearSort = Props.GetDefault("HasClearSort", False)
		bHasClearSort = BANanoShared.parseBool(bHasClearSort)
		
		bHasRefresh = Props.GetDefault("HasRefresh", False)
		bHasRefresh = BANanoShared.parseBool(bHasRefresh)
		
		bHasSearch = Props.GetDefault("HasSearch", False)
		bHasSearch = BANanoShared.parseBool(bHasSearch)
		
		bHasPdf = Props.GetDefault("HasPdf", False)
		bHasPdf = BANanoShared.parseBool(bHasPdf)
		
		bHasExcel = Props.GetDefault("HasExcel", False)
		bHasExcel = BANanoShared.parseBool(bHasExcel)
		
		bHasFilter = Props.GetDefault("HasFilter", False)
		bHasFilter = BANanoShared.parseBool(bHasFilter)
		
		bSpacerBeforeSearch = Props.GetDefault("SpacerBeforeSearch", False)
		bSpacerBeforeSearch = BANanoShared.parseBool(bSpacerBeforeSearch)
		bSpacerAfterSearch = Props.getdefault("SpacerAfterSearch", False)
		bSpacerAfterSearch = BANanoShared.parseBool(bSpacerAfterSearch)
	End If
	'
	bAbsolute = BANanoShared.parseBool(bAbsolute)
	bBottom = BANanoShared.parseBool(bBottom)
	bCollapse = BANanoShared.parseBool(bCollapse)
	bDark = BANanoShared.parseBool(bDark)
	bDense = BANanoShared.parseBool(bDense)
	bExtended = BANanoShared.parseBool(bExtended)
	bFlat = BANanoShared.parseBool(bFlat)
	bFloating = BANanoShared.parseBool(bFloating)
	bLight = BANanoShared.parseBool(bLight)
	bOutlined = BANanoShared.parseBool(bOutlined)
	bProminent = BANanoShared.parseBool(bProminent)
	bShaped = BANanoShared.parseBool(bShaped)
	bShort = BANanoShared.parseBool(bShort)
	bTile = BANanoShared.parseBool(bTile)
	bHidden = BANanoShared.parseBool(bHidden)
	'
	'build and get the element
	If BANano.Exists($"#${mName}"$) Then
		mElement = BANano.GetElement($"#${mName}"$)
	Else	
		mElement = mTarget.Append($"<v-toolbar ref="${mName}" id="${mName}"></v-toolbar>"$).Get("#" & mName)
	End If
	'
	VElement.Initialize(mCallBack, mName, mName)
	VElement.TagName = "v-toolbar"
	'
	If bHasTitle Then
		'add toolbar title
		VElement.Append($"<v-toolbar-title ref="${mName}title" id="${mName}title">{{ ${xTitle} }}</v-toolbar-title>"$)
		'add a div that will appear in small devices
		'VElement.Append($"<v-row id="${mName}titlediv"></v-row>"$)
		'VElement.GetVueElement($"${mName}titlediv"$).HiddenMDAndUp
		VElement.SetData(xTitle, sTitle)
	End If

	If bSpacerBeforeSearch Then AddSpacer
	If bHasSearch Then
		AddSearch
		VElement.SetData(sSearchVModel, "")
		'add a div that will appear in small devices
		'VElement.Append($"<v-row id="${mName}searchdiv"></v-row>"$)
		'VElement.GetVueElement($"${mName}searchdiv"$).HiddenMDAndUp
	End If
	If bSpacerAfterSearch Then AddSpacer
		
	VElement.Classes = mClasses
	VElement.Styles = mStyles
	VElement.Attributes = mAttributes
	VElement.Absolute = bAbsolute
	VElement.Bottom = bBottom
	VElement.AddAttr(":collapse", bCollapse)
	VElement.Bind("color", xColor)
	VElement.SetData(xColor, VElement.BuildColor(sColor, sColorIntensity))
	VElement.Dark = bDark
	VElement.Dense = bDense
	VElement.Elevation = sElevation
	VElement.Extended = bExtended
	VElement.AddAttr("extension-height", sExtensionHeight)
	VElement.Flat = bFlat
	VElement.Floating = bFloating
	VElement.Height = sHeight
	VElement.Light = bLight
	VElement.MaxHeight = sMaxHeight
	VElement.MaxWidth = sMaxWidth
	VElement.MinHeight = sMinHeight
	VElement.MinWidth = sMinWidth
	VElement.Outlined = bOutlined
	VElement.Prominent = bProminent
	VElement.AddClass(sRounded)
	VElement.Shaped = bShaped
	VElement.AddAttr(":short", bShort)
	VElement.Src = sSrc
	VElement.TextColor = sTextColor
	VElement.TextColorIntensity = sTextColorIntensity
	VElement.Tile = bTile
	VElement.VIf = sVIf
	If bHidden Then
	VElement.VShow = sVShow
	VElement.SetData(sVShow, Not(bHidden))
	End If
	VElement.Width = sWidth
	VElement.setAlign(mAlign)	
	VElement.setJustify(mJustify)
	VElement.BindAllEvents
	'
	If bHasAddnew Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_add"$, "mdi-plus", "blue")
	End If
	
	If bHasClearSort Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_clearsort"$, "mdi-sort-variant-remove", "orange")
	End If
	
	If bHasFilter Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_clearfilter"$, "mdi-filter-remove", "red")
	End If
	
	If bHasPdf Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_pdf"$, "mdi-file-pdf-outline", "brown")
	End If
	'
	If bHasExcel Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_excel"$, "mdi-file-excel-box-outline", "cyan")
	End If
	
	If bHasRefresh Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_refresh"$, "mdi-reload", "purple")
	End If
	
	If bHasBack Then
		If bDividersBetween Then AddDivider
		AddButtonIcon($"${mName}_back"$, "mdi-chevron-left", "cyan")
	End If
End Sub

