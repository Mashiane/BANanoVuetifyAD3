'add a search to the card title
Sub AddSearch
Dim str As String = $"<v-text-field id="${mName}search" @keydown.native.escape="${sSearchVModel}=''" v-model="${sSearchVModel}" append-icon="mdi-magnify" label="Search" single-line hide-details clearable></v-text-field>"$
Dim ct As BANanoElement
ct.Initialize($"#${mName}"$)
ct.Append(str)
Dim txtSearch As VueElement = GetSearchText
txtSearch.Rounded = True
txtSearch.BindAllEvents
txtSearch.Dense = True
txtSearch.Solo = True
VElement.BindVueElement(txtSearch)
End Sub
Sub GetSearchText As VueElement
Dim el As VueElement
el.Initialize(mCallBack, $"${mName}search"$, $"${mName}search"$)
Return el
End Sub
'return html of the element
Sub getHTML As String
If mElement <> Null Then
Return mElement.GetHTML
Else
Return ""
End If
End Sub
