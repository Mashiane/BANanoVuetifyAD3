VElement.AddAttr(":value", sValue)
VElement.Width = sWidth
If bUsesAuthentication Then
	sVIf = "authenticated"
End If
VElement.VIf = sVIf
If bHidden Then
	VElement.VShow = sVShow
	VElement.SetData(sVShow, Not(bHidden))
End If
VElement.BindAllEvents
End Sub

'show the progress loader
Sub Pause(C As VueComponent)
	  C.SetData(ProgressLoader, True)
End Sub

'hide the progress loader
Sub Resume(C As VueComponent)
	C.SetData(ProgressLoader, False)
End Sub

'show the progress loader on app
Sub PauseOnApp(V As VuetifyApp)
	  V.SetData(ProgressLoader, True)
End Sub

'hide the progress loader on app
Sub ResumeOnApp(V As VuetifyApp)
	V.SetData(ProgressLoader, False)
End Sub

Sub AddToParent(targetID As String)
	mTarget = BANano.GetElement("#" & targetID.ToLowerCase)
	DesignerCreateView(mTarget, Null)
End Sub

Sub Remove()
	mElement.Remove
	BANano.SetMeToNull
End Sub


Sub AddClass(s As String) As VAppBar
	VElement.AddClass(s)
	Return Me
End Sub

Sub AddAttr(p As String, v As Object) As VAppBar
	VElement.SetAttr(p, v)
	Return Me
End Sub

Sub AddStyle(p As String, v As String) As VAppBar
	VElement.AddStyle(p, v)
	Return Me
End Sub

Sub RemoveAttr(p As String) As VAppBar
	VElement.RemoveAttr(p)
	Return Me
End Sub

Sub UpdateVisible(C As VueComponent, b As Boolean) As VAppBar
	C.SetData(sVIf, b)
	C.SetData(sValue, b)
	Return Me
End Sub

Sub UpdateColor(C As VueComponent, vColor As String, vIntensity As String)
	sColor = VElement.BuildColor(vColor, vIntensity)
	C.SetData(xColor, sColor)
End Sub

Sub UpdateVisibleOnApp(C As VuetifyApp, b As Boolean) As VAppBar
	C.SetData(sVIf, b)
	C.SetData(sValue, b)
	Return Me
End Sub

Sub UpdateColorOnApp(C As VuetifyApp, vColor As String, vIntensity As String)
	sColor = VElement.BuildColor(vColor, vIntensity)
	C.SetData(xColor, sColor)
End Sub

Sub getID As String
	Return mName
End Sub


Sub getHere As String
	Return $"#${mName}"$
End Sub


Sub BindState(C As VueComponent)
	VC = c
	Dim mbindings As Map = VElement.bindings
	Dim mmethods As Map = VElement.methods
	'apply the binding for the control
	For Each k As String In mbindings.Keys
		Dim v As Object = mbindings.Get(k)
		Select Case k
		Case "key"
		Case Else
			C.SetData(k, v)
		End Select
	Next
	'apply the events
	For Each k As String In mmethods.Keys
		Dim cb As BANanoObject = mmethods.Get(k)
		C.SetCallBack(k, cb)
	Next
End Sub

Sub BindStateOnApp(c As VuetifyApp)
	Dim mbindings As Map = VElement.bindings
	Dim mmethods As Map = VElement.methods
	'apply the binding for the control
	For Each k As String In mbindings.Keys
		Dim v As Object = mbindings.Get(k)
		Select Case k
		Case "key"
		Case Else
			C.SetData(k, v)
		End Select
	Next
	'apply the events
	For Each k As String In mmethods.Keys
		Dim cb As BANanoObject = mmethods.Get(k)
		C.SetCallBack(k, cb)
	Next
End Sub

